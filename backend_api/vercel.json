{
    "version": 2,
    "builds": [
        {
            "src": "psite/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "installCommand": "pip install -r requirements.txt && python manage.py collectstatic --noinput"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "psite/wsgi.py"
        }
    ]
}